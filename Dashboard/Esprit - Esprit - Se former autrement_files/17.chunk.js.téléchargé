/**
 * @param {String} endpoint relative endpoint
 * @param {object} body request body
 * @param {String} method method can be ["GET","POST","PUT", "DELETE"] | Default GET
 * @param {boolean} transformBody whether to transform the request body from JSON to FormData | Default false
 */

function useApi(endpoint) {
  let body = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : null;
  let method = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : "GET";
  let transformBody = arguments.length > 3 && arguments[3] !== undefined ? arguments[3] : false;
  const [result, setResult] = Object(react__WEBPACK_IMPORTED_MODULE_0__["useState"])(null);
  const [error, setError] = Object(react__WEBPACK_IMPORTED_MODULE_0__["useState"])(null);

  async function query() {
    if (!endpoint) return;
    setError(false);
    setResult(null);
    const [res, err] = await Object(_utils_queryApi__WEBPACK_IMPORTED_MODULE_1__["queryApi"])(endpoint, body, method, transformBody);
    setResult(res);
    setError(err);
  }

  Object(react__WEBPACK_IMPORTED_MODULE_0__["useEffect"])(() => {
    query(); // eslint-disable-next-line
  }, []);
  return [result, error, query];
}

