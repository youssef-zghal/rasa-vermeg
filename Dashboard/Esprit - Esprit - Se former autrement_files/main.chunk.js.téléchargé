/***/ "./src/components/utils/isExternal.js":
/*!********************************************!*\
  !*** ./src/components/utils/isExternal.js ***!
  \********************************************/
/*! exports provided: isExternal */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "isExternal", function() { return isExternal; });
var checkDomain = function (url) {
  if (url.indexOf("//") === 0) {
    url = window.location.protocol + url;
  }

  return url.toLowerCase().replace(/([a-z])?:\/\//, "$1").split("/")[0];
};

function isExternal(url) {
  return url && (url.indexOf(":") > -1 || url.indexOf("//") > -1) && checkDomain(window.location.href) !== checkDomain(url);
}

